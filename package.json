    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "guid": "0.0.12",
    "jsonwebtoken": "^8.4.0",
    "mysql2": "^1.6.4",
    "node-fetch": "^2.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
